{
  "name": "monaco-docsy",
  "version": "0.4.0",
  "description": "Docsy syntax highlighting for monaco-editor",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/monaco-docsy#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/monaco-docsy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/monaco-docsy.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/monaco-docsy.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsdx build --target web",
    "lint": "tsdx lint src test",
    "pub": "yarn build && np",
    "start": "tsdx watch",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ]
  },
  "dependencies": {
    "monaco-editor": "0.22.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "np": "^7.4.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}
