{
  "name": "monaco-docsy",
  "version": "0.5.0",
  "description": "Docsy syntax highlighting for monaco-editor",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/monaco-docsy#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/monaco-docsy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/monaco-docsy.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "type": "module",
  "exports": "./dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p ./tsconfig.build.json",
    "example:run": "vite example",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@monaco-editor/loader": "^1.1.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.5",
    "jest-ts-tsx-resolver": "^1.0.0",
    "monaco-editor": "^0.25.0",
    "np": "^7.5.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4",
    "vite": "^2.3.8"
  },
  "peerDependencies": {
    "monaco-editor": "^0.25.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
